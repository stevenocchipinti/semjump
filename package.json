{
  "name": "semantic-jump-extension",
  "version": "1.0.0",
  "description": "Tests for the Semantic Jump Chrome Extension",
  "private": true,
  "scripts": {
    "test": "node --experimental-vm-modules ./node_modules/.bin/jest"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.5.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {},
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).?(m)[tj]s?(x)"
    ],
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ]
  }
}
